<MyLayout>:
    name: "main-page"
    ScrollView:
        FloatLayout:
            size: root.width, root.height*2.3
            size_hint_y: None
            canvas:
                Color:
                    rgba: (0, 0, 0, 1)
            Button:
                font_size: 15
                color: (144/255, 66/255, 11/255, 1)
                background_color: (1, 1, 1, 0)
                size_hint: 0.1, 0.05
                pos_hint: {"x": 0.05, "top": 1}
                text: "Booking"
                on_press: self.color = (77/255, 33/255, 2/255, 1)
                on_release:
                    self.color = (144/255, 66/255, 11/255, 1)
                    root.set_payment_type()
                    app.root.current = "booking-guest"
                    root.manager.transition.direction = 'left'

            Button:
                font_size: 15
                color: (144/255, 66/255, 11/255, 1)
                background_color: (1, 1, 1, 0)
                size_hint: 0.1, 0.05
                pos_hint: {"x": 0.85, "top": 1}
                text: "Log In"
                on_press: self.color = (77/255, 33/255, 2/255, 1)
                on_release:
                    self.color = (144/255, 66/255, 11/255, 1)
                    app.root.current = "login"
                    root.manager.transition.direction = 'left'

            Label:
                font_size: 32
                color: (144/255, 66/255, 11/255, 1)
                size_hint: None, None
                size: self.texture_size
                pos_hint: {"center_x": 0.5, "top": 0.99}
                text: "HOTEL"

            Image:
                source: 'header-bg.jpeg'
                pos_hint: {'left': 1, 'top': 0.95}
                size_hint: None, None
                size: root.width, 600
                allow_stretch: True
                keep_ratio: False

            Label:
                font_size: 32
                color: (1, 1, 1, 1)
                size_hint: None, None
                size: self.texture_size
                pos_hint: {"center_x": 0.5, "top": 0.81}
                text: "MANAGE YOUR BOOKING ANYTIME ANYWHERE"
            
            Button:
                font_size: 28
                color: (144/255, 66/255, 11/255, 1)
                background_color: (250, 182, 37, 1)
                size_hint: 0.15, 0.03
                pos_hint: {"center_x": 0.5, "top": 0.75}
                text: "Search..."
                on_press: self.color = (77/255, 33/255, 2/255, 1)
                on_release:
                    self.color = (144/255, 66/255, 11/255, 1)
                    root.set_payment_type()
                    app.root.current = "booking-guest"
                    root.manager.transition.direction = 'left'
            
            Label:
                font_size: 32
                color: (144/255, 66/255, 11/255, 1)
                size_hint: None, None
                size: self.texture_size
                pos_hint: {"center_x": 0.5, "top": 0.56}
                text: "Comfortable Rooms"
                
            GridLayout:
                pos_hint: {"x": 0, "top": 0.56}
                cols: 3
                size_hint: (None, None)
                size: root.width, 350
                
                GridLayout:
                    cols: 1
                    BoxLayout:
                        orientation: 'vertical'
                        padding: 10
                        Image:
                            source: 'standardroom.png'

                                app.root.current = "standard-room"
                                root.manager.transition.direction = 'left'
                GridLayout:
                    cols: 1
                    BoxLayout:
                        orientation: 'vertical'
                        padding: 10
                        Image:
                            source: 'juniorsuite.png'
                            size_hint: None, None
                            size: root.width/3, 250
                            pos_hint: {"center_x": 0.5}
                            allow_stretch: True
                        
                        Button:
                            text: "Junior Suite"
                            color: 1, 1, 1, 1
                            background_normal: ''
                            background_color: (186/255, 135/255, 81/255, 1)
                            size_hint: None, None
                            size: root.width/5, 30
                            pos_hint: {"center_x": 0.5}
                            on_release:
                                app.root.current = "junior-suite"
                                root.manager.transition.direction = 'left'
                GridLayout:
                    cols: 1
                    BoxLayout:
                        orientation: 'vertical'
                        padding: 10
                        Image:
                            source: 'kingsuite.png'
                            size_hint: None, None
                            size: root.width/3, 250
                            pos_hint: {"center_x": 0.5}
                            allow_stretch: True
                        
                        Button:
                            text: "King Suite"
                            color: 1, 1, 1, 1
                            background_normal: ''
                            background_color: (186/255, 135/255, 81/255, 1)
                            size_hint: None, None
                            size:root.width/5, 30
                            pos_hint: {"center_x": 0.5}
                            # on_press: root.show(self)
                            on_release:
                                app.root.current = "king-suite"
                                root.manager.transition.direction = 'left'
            GridLayout:
                pos_hint: {"x": 0, "top": 0.32}
                cols: 2
                size_hint: (None, None)
                size: root.width, 350
                GridLayout:
                    cols: 1
                    BoxLayout:
                        orientation: 'vertical'
                        padding: 10
                        Image:
                            source: 'superiorsuite.png'
                            size_hint: None, None
                            size: root.width/2.3, 300
                            pos_hint: {"center_x": 0.5}
                            allow_stretch: True
                        
                        Button:
                            text: "Superior Suite"
                            color: 1, 1, 1, 1
                            background_normal: ''
                            background_color: (186/255, 135/255, 81/255, 1)
                            size_hint: None, None
                            size: root.width/4, 30
                            pos_hint: {"center_x": 0.5}
                            # on_press: root.show(self)
                            on_release:
                                app.root.current = "superior-suite"
                                root.manager.transition.direction = 'left'
                GridLayout:
                    cols: 1   

                pos_hint: {"x": 0, "top": 0.07}
                height: 0.5

            Label:
                font_size: 16
                color: (144/255, 66/255, 11/255, 1)
                size_hint: None, None
                size: self.texture_size
                pos_hint: {"x": 0.05, "top": 0.065}
                text: "Contacts:\n\nTole bi 59, Almaty, Kazakhstan\nreservation@hotel.kz"
<LogInWindow>:
    name: "login"

    login: login
    password: password
    login_btn: login_btn

    FloatLayout:
        size: root.width, root.height

        Image:
            source: 'kazakhstan_hotel.jpg'
            pos_hint: {'x': 0, 'y': 0}
            size_hint: None, None
            size: root.width, root.height
            allow_stretch: True
            keep_ratio: False

        Image:
            source: 'white.jpg'
            opacity: 0.5
            pos_hint: {'x': 300/1200, 'center_y': 0.5}
            size_hint: 600/1200, 600/800
            allow_stretch: True
            keep_ratio: False

    FloatLayout:
        size_hint: 600/1200, 800/800
        pos_hint: {"x": 300/1200}

        canvas:
            Color:
                rgba: (144/255, 66/255, 11/255, 1)
            # Line:
            #     width: 1.2
            #     points: (300, 0, 300, 800, 900, 800, 900, 0)


        Label:
            text:"Login: "
            color: (144/255, 66/255, 11/255, 1)
            font_size: 28
            bold: True
            pos_hint: {"x":0.1, "top":0.8}
            size_hint: 0.35, 0.15

        TextInput:
            id: login
            font_size: 18
            multiline: False
            pos_hint: {"x": 0.45 , "top":0.75}
            size_hint: 0.45, 0.05

        Label:
            text:"Password: "
            color: (144/255, 66/255, 11/255, 1)
            font_size: 28
            bold: True
            pos_hint: {"x":0.1, "top":0.7}
            size_hint: 0.35, 0.15

        TextInput:
            id: password
            font_size: 18
            multiline: False
            password: True
            pos_hint: {"x": 0.45, "top":0.65}
            size_hint: 0.45, 0.05

        Button:
            id: back_btn4
            background_color: (186/255, 135/255, 81/255, 1)
            background_normal: ''
            pos_hint:{"center_x": 0.5,"y":0.35}
            size_hint: 0.5, 0.08
            font_size: 24
            text: "Back"
            on_release:
                color = (186/255, 135/255, 81/255, 1)
                login.text = ''
                password.text = ''
                app.root.current = "main-page"
                root.manager.transition.direction = 'right'

        Button:
            id: login_btn
            color: (1, 1, 1, 1)
            background_color: (186/255, 135/255, 81/255, 1)
            background_normal: ''
            pos_hint:{"center_x":0.5,"y":0.25}
            size_hint: 600/1200, 0.08
            font_size: 24
            text: "Login"
            on_release:
                root.emp_operation()
                root.manager.transition.direction = "up"


<RoomInfoUpd>:
    title: "Price Change"
    size_hint: None, None
    size: 400, 270
    title_align: "center"
    auto_dismiss: False

    GridLayout:
        cols: 1
        padding: 10
        spacing: 10

        BoxLayout:
            orientation: "horizontal"
            spacing: 0.1

            Label:
                color: (1, 1, 1, 1)
                size_hint: 0.3, 0.35
                pos_hint: {"y": 0.35}
                text: "New Price:"
            TextInput:
                id: txtinput
                size_hint: 0.3, 0.35
                pos_hint: {"y": 0.35}
                text: root.col_data[2]

        BoxLayout:
            orientation: "horizontal"
            spacing: 5

            Button:
                text: "Save"
                size_hint: 0.3, 0.35
                pos_hint: {"y": 0.1}
                on_release:
                    root.obj.update_changes(root.col_data[0], root.col_data[1], txtinput.text)
            Button:
                text: "Back"
                size_hint: 0.3, 0.35
                pos_hint: {"y": 0.1}
                on_release: root.dismiss()

